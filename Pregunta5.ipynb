        "def Suma_Series(n):\n",
        "    if n <= 0:\n",
        "        return 0\n",
        "    else:\n",
        "        return n + Suma_Series(n - 2)\n",
        "\n",
        "n = 6\n",
        "print(Suma_Series(n))\n"
